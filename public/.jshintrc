{
    "passfail"      : false,
    "maxerr"        : 10000,
    "browser"       : true,
    "predef": [
        "Backbone",
        "_",
        "moment",
        "d3",
        "require",
        "module"
    ],
    "devel"         : true,
    "jquery"        : true,
    "strict"        : true,
    "globalstrict"  : true,
    "bitwise"       : true,
    "curly"         : true,
    "eqeqeq"        : true,
    "evil"          : false,
    "forin"         : true,
    "indent"        : 4,
    "latedef"       : true,
    "loopfunc"      : false,
    "newcap"        : true,
    "noarg"         : true,
    "noempty"       : true,
    "quotmark"      : "double",
    "trailing"      : true,
    "undef"         : true,
    "unused"        : "vars",
    "white"         : true,
    "notypeof"      : false,
    "freeze"        : true,
    "camelcase"     : false
}